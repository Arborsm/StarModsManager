<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ui="using:FluentAvalonia.UI.Controls"
             xmlns:vm="using:StarModsManager.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="StarModsManager.Views.NavigationPages"
             x:DataType="vm:NavigationPagesModel">
    
    <ui:NavigationView x:Name="MainNav" PaneDisplayMode="Left" IsPaneOpen="False" >
        <ui:NavigationView.MenuItems>
            <ui:NavigationViewItem Content="Main" Tag="Main" IconSource="Home"/>
            <ui:NavigationViewItem Content="Menu Item2" Tag="SamplePage2" IconSource="Save" />
            <ui:NavigationViewItem Content="Menu Item3" Tag="SamplePage3" IconSource="Refresh" />
            <ui:NavigationViewItem Content="Menu Item4" Tag="SamplePage4" IconSource="Download" />
        </ui:NavigationView.MenuItems>
    </ui:NavigationView>
    
</UserControl>
